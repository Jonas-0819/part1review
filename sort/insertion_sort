def insertion_sort(arr):
    n = len(arr)
    
    for i in range(1, n):
        current = arr[i]
        j = i - 1

        while j >= 0 and arr[j][0] > current[0]:
            arr[j + 1] = arr[j]
            j -= 1
        arr[j + 1] = current
data = [
    (3, 'A'),
    (1, 'B'),
    (3, 'C'),
    (2, 'D'),
    (3, 'E')
]
print("原始資料：")
print(data)
insertion_sort(data)
print("\n排序後資料：")
print(data)